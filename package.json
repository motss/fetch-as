{
  "name": "fetch-as",
  "version": "0.5.0-alpha.1",
  "description": "Fetch API in Node.js with specific response type",
  "keywords": [
    "array-buffer",
    "blob",
    "buffer",
    "fetch",
    "fetch-api",
    "fetch-as",
    "json",
    "node",
    "node-fetch",
    "text",
    "text-converted",
    "typescript"
  ],
  "homepage": "https://github.com/motss/fetch-as",
  "repository": {
    "type": "git",
    "url": "git@github.com:motss/fetch-as.git"
  },
  "license": "MIT",
  "author": {
    "name": "Rong Sen Ng",
    "email": "wes.ngrongsen@gmail.com",
    "url": "https://github.com/motss"
  },
  "files": [
    "index.js",
    "index.mjs",
    "index.d.ts"
  ],
  "main": "./index",
  "scripts": {
    "build": "npm run build:cjs && npm run build:esm && rm -rf dist/ && echo \"> [build] Done!\"",
    "build:cjs": "npm run lint && rm -rf dist/ && tsc -p tsconfig.json && mv dist/index.js dist/index.d.ts . && echo \"> [cjs] Done!\"",
    "build:esm": "npm run lint && rm -rf dist/ && tsc -p tsconfig.esm.json && mv dist/index.js ./index.mjs && echo \"> [esm] Done!\"",
    "demo": "node demo/index.js",
    "lint": "tslint --project tsconfig.json --config tslint.prod.json --format stylish 'src/**/*.ts*'",
    "lint:debug": "tslint --project tsconfig.json --config tslint.json --format stylish 'src/**/*.ts*'",
    "prepublishOnly": "npm run lint && npm run build",
    "test": "jest --detectOpenHandles --config ./.jestrc.json"
  },
  "dependencies": {
    "node-fetch": "^2.2.0"
  },
  "devDependencies": {
    "@messageflow/tslint-config": "^1.1.0",
    "@types/jest": "^23.3.1",
    "@types/nock": "^9.3.0",
    "@types/node": "^10.7.1",
    "@types/node-fetch": "^2.1.2",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "encoding": "^0.1.12",
    "jest": "^23.5.0",
    "nock": "^9.6.1",
    "ts-jest": "^23.1.3",
    "tslint": "^5.11.0",
    "typescript": "^3.0.1"
  },
  "engines": {
    "node": ">= 8.9.0",
    "npm": ">= 5.5.1"
  }
}
