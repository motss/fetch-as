{
  "name": "fetch-as",
  "version": "0.1.0",
  "description": "Fetch API in Node.js with specific response type",
  "keywords": [
    "array-buffer",
    "blob",
    "buffer",
    "fetch",
    "fetch-api",
    "fetch-as",
    "json",
    "node",
    "node-fetch",
    "text",
    "text-converted",
    "typescript"
  ],
  "homepage": "https://github.com/motss/fetch-as",
  "license": "MIT",
  "author": {
    "name": "Rong Sen Ng",
    "email": "wes.ngrongsen@gmail.com",
    "url": "https://github.com/motss"
  },
  "main": "./index.js",
  "repository": {
    "type": "git",
    "url": "git@github.com:motss/fetch-as.git"
  },
  "scripts": {
    "build": "npm run ts && cross-env NODE_ENV=production gulp build && npm run cleanup",
    "build:debug": "npm run ts && gulp build && npm run cleanup",
    "check": "npm run lint && npm run nsp",
    "cleanup": "rm -rf gulpfile.js",
    "gulp": "npm run ts && gulp",
    "lint": "tslint --project tsconfig.json --config tslint.json",
    "lint:fix": "tslint -p . -c tslint.json --fix",
    "nsp": "nsp check",
    "prepublishOnly": "npm run check && npm run build",
    "test": "jest --config ./.jestrc.json",
    "ts": "tsc --target esnext --lib esnext,esnext.asynciterable --module commonjs gulpfile.ts"
  },
  "dependencies": {
    "node-fetch": "^1.7.3"
  },
  "devDependencies": {
    "@types/jest": "^22.0.1",
    "@types/node": "^8.5.7",
    "@types/node-fetch": "^1.6.7",
    "babel-cli": "^6.26.0",
    "babel-plugin-transform-function-bind": "^6.22.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-minify": "^0.2.0",
    "codecov": "^3.0.0",
    "coveralls": "^3.0.0",
    "cross-env": "^5.1.1",
    "del": "^3.0.0",
    "env-cmd": "^7.0.0",
    "gulp": "^3.9.1",
    "gulp-babel": "^7.0.0",
    "gulp-sequence": "^1.0.0",
    "gulp-tslint": "^8.1.2",
    "gulp-typescript": "^3.2.3",
    "jest": "^22.0.4",
    "nock": "^9.1.5",
    "nsp": "^3.1.0",
    "ts-jest": "^22.0.1",
    "tslint": "^5.8.0",
    "tslint-config-airbnb": "^5.4.2",
    "tslint-consistent-codestyle": "^1.11.0",
    "tslint-immutable": "^4.5.0",
    "typescript": "^2.6.2"
  },
  "engines": {
    "node": ">= 8.9.0",
    "npm": ">= 5.5.1"
  },
  "@std/esm": "cjs"
}
